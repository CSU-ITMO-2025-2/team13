apiVersion: v1
kind: Secret
metadata:
  name: team13-app-secrets
  namespace: team13-ns
type: Opaque
stringData:
  postgres-password: "your_database_password_here"
  rabbitmq-password: "your_rabbitmq_password_here"
  secret-key: "secret_key_here"
  gemini-api-key: "your_gemini_api_key_here"
  proxy-url: "http://user:password@ip:port"